<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:paddingBottom="@dimen/activity_vertical_margin"
	android:paddingLeft="@dimen/activity_horizontal_margin"
	android:paddingRight="@dimen/activity_horizontal_margin"
	android:paddingTop="@dimen/activity_vertical_margin"
	tools:context="com.example.amit_calculator.MainActivity$PlaceholderFragment" >

	
	
	<TextView
    	android:id="@+id/editText1"
    	android:layout_width="220dp"
    	android:layout_height="50dp"
    	android:layout_alignParentLeft="true"
    	android:layout_alignParentTop="true"
    	android:text="@string/hello_world"
    	android:textSize="22sp" />

	<Button
		android:id="@+id/button1"
		style="?android:attr/buttonStyleSmall"
		android:layout_width="70dp"
		android:layout_height="50dp"
		android:layout_alignLeft="@+id/editText1"
		android:layout_below="@+id/editText1"
		android:layout_marginTop="21dp"
		android:text="1" />

	<Button
		android:id="@+id/button2"
		style="?android:attr/buttonStyleSmall"
		android:layout_width="70dp"
		android:layout_height="50dp"
		android:layout_alignParentRight="true"
		android:layout_alignParentTop="true"
		android:text="del" />

		<Button
		android:id="@+id/button3"
		style="?android:attr/buttonStyleSmall"
		android:layout_width="70dp"
		android:layout_height="50dp"
		android:layout_alignTop="@+id/button1"
		android:layout_toRightOf="@+id/button1"
		android:text="2" />
	

	<Button
		android:id="@+id/button6"
		style="?android:attr/buttonStyleSmall"
		android:layout_width="70dp"
		android:layout_height="50dp"
		android:layout_alignBottom="@+id/button5"
		android:layout_toLeftOf="@+id/button4"
		android:text="5" />

	<Button
		android:id="@+id/button7"
		style="?android:attr/buttonStyleSmall"
				android:layout_width="70dp"
		android:layout_height="50dp"
		android:layout_alignLeft="@+id/button4"
		android:layout_alignTop="@+id/button6"
		android:text="6" />
		
	
	

	<Button
		android:id="@+id/button5"
		style="?android:attr/buttonStyleSmall"
				android:layout_width="70dp"
		android:layout_height="50dp"
		android:layout_below="@+id/button1"
		android:layout_toLeftOf="@+id/button3"
		android:text="4" />

	<Button
		android:id="@+id/button4"
		style="?android:attr/buttonStyleSmall"
				android:layout_width="70dp"
		android:layout_height="50dp"
				android:layout_above="@+id/button6"
		android:layout_toRightOf="@+id/button3"
		android:text="3" />

	<Button
		android:id="@+id/button8"
		style="?android:attr/buttonStyleSmall"
				android:layout_width="70dp"
		android:layout_height="50dp"
		android:layout_below="@+id/button6"
		android:layout_toLeftOf="@+id/button6"
		android:text="7" />

	<Button
		android:id="@+id/button9"
		style="?android:attr/buttonStyleSmall"
				android:layout_width="70dp"
		android:layout_height="50dp"
		android:layout_alignBaseline="@+id/button8"
		android:layout_alignBottom="@+id/button8"
		android:layout_toRightOf="@+id/button5"
		android:text="8" />

	<Button
		android:id="@+id/button10"
		style="?android:attr/buttonStyleSmall"
				android:layout_width="70dp"
		android:layout_height="50dp"
		android:layout_alignBaseline="@+id/button9"
		android:layout_alignBottom="@+id/button9"
		android:layout_toRightOf="@+id/button6"
		android:text="9" />

	<Button
		android:id="@+id/button11"
		style="?android:attr/buttonStyleSmall"
				android:layout_width="70dp"
		android:layout_height="50dp"
		android:layout_alignLeft="@+id/button2"
		android:layout_alignTop="@+id/button4"
		android:text="+" />

	<Button
		android:id="@+id/button12"
		style="?android:attr/buttonStyleSmall"
				android:layout_width="70dp"
		android:layout_height="50dp"
		android:layout_alignLeft="@+id/button11"
		android:layout_below="@+id/button11"
		android:text="-" />

	<Button
		android:id="@+id/button13"
		style="?android:attr/buttonStyleSmall"
				android:layout_width="70dp"
		android:layout_height="50dp"
		android:layout_alignBaseline="@+id/button10"
		android:layout_alignBottom="@+id/button10"
		android:layout_alignLeft="@+id/button12"
		android:text="*" />

	<Button
		android:id="@+id/button14"
		style="?android:attr/buttonStyleSmall"
				android:layout_width="70dp"
		android:layout_height="50dp"
		android:layout_alignLeft="@+id/button8"
		android:layout_below="@+id/button8"
		android:text="." />

	<Button
		android:id="@+id/button15"
		style="?android:attr/buttonStyleSmall"
				android:layout_width="70dp"
		android:layout_height="50dp"
		android:layout_alignTop="@+id/button14"
		android:layout_toLeftOf="@+id/button10"
		android:text="0" />

	<Button
		android:id="@+id/button18"
		style="?android:attr/buttonStyleSmall"
				android:layout_width="70dp"
		android:layout_height="50dp"
		android:layout_alignLeft="@+id/button14"
		android:layout_below="@+id/button14"
		android:text="^" />
	
	<Button
		android:id="@+id/button19"
		style="?android:attr/buttonStyleSmall"
				android:layout_width="70dp"
		android:layout_height="50dp"
		android:layout_alignTop="@+id/button18"
		android:layout_toRightOf="@+id/button18"
		android:text="(" />
	
	<Button
		android:id="@+id/button20"
		style="?android:attr/buttonStyleSmall"
				android:layout_width="70dp"
		android:layout_height="50dp"
		android:layout_alignTop="@+id/button19"
		android:layout_toRightOf="@+id/button19"
		android:text=")" />
	
	<Button
		android:id="@+id/button21"
		android:layout_width="107dp"
		android:layout_height="50dp"
		android:layout_alignTop="@+id/button20"
		android:layout_toRightOf="@+id/button20"
		android:text="=" />
	

	<Button
		android:id="@+id/button16"
		style="?android:attr/buttonStyleSmall"
				android:layout_width="70dp"
		android:layout_height="50dp"
		android:layout_alignRight="@+id/button10"
		android:layout_alignTop="@+id/button15"
		android:text="00" />

	<Button
		android:id="@+id/button17"
		style="?android:attr/buttonStyleSmall"
				android:layout_width="70dp"
		android:layout_height="50dp"
		android:layout_alignLeft="@+id/button13"
		android:layout_alignTop="@+id/button16"
		android:text="/" />



	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical" >

		<FrameLayout
			android:id="@android:id/tabcontent"
			android:layout_width="match_parent"
			android:layout_height="438dp" >
		</FrameLayout>

	</LinearLayout>



</RelativeLayout>